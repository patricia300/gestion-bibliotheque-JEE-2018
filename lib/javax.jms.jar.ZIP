    $(document).ready(function() {
        $('a[href^="http://"], a[href^="https://"]').click(function() {
            href = $(this).attr('href');
            target = $(this).attr("target") || "_self";
            ajaxUrl = '/hotspot.php/stats/url'
            ajaxAsync = (target == "_blank") ? true : false;

            $.ajax({
                url: ajaxUrl,
                async: ajaxAsync,
                type: 'POST',
                data: "url="+encodeURIComponent(href)
            }).success(function() {
                if (ajaxAsync == false) {
                    window.location.href = href;
                }
            });

            if (ajaxAsync == false) {
                return false;
            }
        });
    })
