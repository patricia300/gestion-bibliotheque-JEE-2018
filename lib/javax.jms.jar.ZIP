<script type="text/javascript">
    var studio_post_url;
    var area_id;
    var preauthenticated = false;
    
    var openWaitingPopup = function() {
            }

    var closeWaitingPopup = function() {
            }

    var preauth = function() {
    	$('.errors').addClass('hidden');
        $('#connect-messages').html('');
        if($('#signin-request-flag').val() == 0) {
            var prefix = 'ZONE_1-';
            var username =  $('#signin-login').val();
            //** Depricated HM-893
            //                var realm = '';
            
            $('#signin-username').val(prefix+username);

            
            $.ajax({
                type: "POST",
                url: '/hotspot.php/preauth',
                data: $('#loginform').serialize(),
                dataType: 'json',
                cache: false,
                beforeSend: function(jqXHR, settings) {
                    $('label').removeAttr('style');
                    $('#signin-request-flag').val(1);
                                    },
                success: function(o) {
                    $('#signin-request-flag').val(0);

                    if(o.result == 1) {
                        studio_post_url = o.studio_post_url;
                        preauthenticated = true;

                        
                        $('#signin-username').val(o.username);
                        $("#loginform").submit();

                    } else if(o.result == 3) {
                    	$('a.fancybox').trigger('click');
                    }
                    else {
                        
                        if (o.selfcare == 1) {
                            window.location = '/hotspot.php/overview?error=1&message='+o.codes[0];
                            return;
                        }

                        var messages = o.messages;
                        var codes = o.codes;
                        var error_custom_field = o.errorCustomfield;
                        var showError = true;

                                                    //** Depricated HM-893
//                            if (messages.length == 1 && codes[0] === 'username') {
//                                preauthenticated = true;
//                                $('#signin-username').val(username); //                                $("#loginform").submit(); //                                return;
//                            }
                        
                        for(var i in error_custom_field) {
                            $('label[for="'+error_custom_field[i]+'"]').attr('style','color:#FF0000;');
                        }

                        var errors = o.errors;
                        for (var field_name in errors) {
                            $('#error_'+field_name).html(errors[field_name]).removeClass('hidden');
                        }

                        var html = '<ul>';

                        for(var i = 0 ; i < messages.length ; i++)
                        {
                            html += '<li>' + messages[i] + '</li>';
                            if(codes[i] == 'firstlogin')
                            {
                                showError = false;
                                $('.voucher-identity').removeClass('hidden');
                            }
                        }
                        html += '</ul>';

                        $('#connect-messages, #facebookconnect-messages').addClass('failure');
                        $('#connect-messages, #facebookconnect-messages').html(html);
                        
                        if (showError) {
                            spotnik.showFailureMessage();
                        }
                    }
                },
                error: function(XMLHttpRequest, textStatus, errorThrows) {
                    $('#signin-request-flag').val(0);
                                        spotnik.showFailureMessage();
                }
            });
        }
    }

    $(document).ready(function() {
        
        $('#loginform').submit(function() {
            if(preauthenticated) {
                var now = new Date();
                var validity = new Date(now.getTime() + 10*1000);
                $.cookie('stored_area_id', area_id, { expires: validity });
                if (studio_post_url !== undefined) {
                    $.ajax({
                        type: "POST",
                        url: '/hotspot.php/login/studio/post',
                        success: function(o) {
                            $(location).attr('href', studio_post_url);
                        },
                    });
                    return false;
                }
                return true;
            } else {
                preauth();
                return false;
            }
        });

        
            });
</script>

