<script type="text/javascript">
    $(document).ready(function() {
        var newInput;
        var inputPasswordId = "signin-password";
        var inputPasswordConfirmId = false;
        var showPassword = $("#show_password");

        if (!$.browser.msie) {
            if (showPassword.length != 0 && $("#"+inputPasswordId).length != 0) {
                showPassword.change(function() {
                    inputPassword = $("#"+inputPasswordId);

                    if (showPassword.is(':checked')) {
                        newInput = inputPassword.clone(true).attr('type', 'text');
                        if (inputPasswordConfirmId != false) {
                            newInputConfirm = $("#"+inputPasswordConfirmId).clone().attr('type', 'text');
                        }
                    } else {
                        newInput = inputPassword.clone(true).attr('type', 'password');
                        if (inputPasswordConfirmId != false) {
                            newInputConfirm = $("#"+inputPasswordConfirmId).clone().attr('type', 'password');
                        }
                    }

                    inputPassword.replaceWith(newInput);
                    if (inputPasswordConfirmId != false) {
                        $("#"+inputPasswordConfirmId).replaceWith(newInputConfirm);
                    }
                });
                showPassword.trigger('change');
            }
        } else {
            $('li#li-show-password').hide();
        }

    });
</script></div>

